ALTER proc [dbo].[Messages_Delete_ById]
				@Id int

AS
/*
Declare @Id int = 10

Execute dbo.Messages_Delete_ById @Id

****
Execute dbo.Messages_SelectAll

*/

BEGIN

DELETE FROM [dbo].[Messages]
      WHERE Id = @Id


END
